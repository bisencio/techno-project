{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/privacy",
      "dest": "/api/privacy.js"
    },
    {
      "src": "/terms",
      "dest": "/api/terms.js"
    },
    {
      "src": "/deletion",
      "dest": "/api/deletion.js"
    }
  ]
}